\name{contrastAnalysis}
\alias{contrastAnalysis}
\alias{contrastAnalysis.default}
\title{Contrast Analysis}
\description{
 Perform Contrast Analysis
}
\usage{
contrastAnalysis(model, contrastOpt = c("control", "user"), controlLevel = NULL, contrast = NULL, alpha = 0.05)
}
\arguments{
  \item{model}{an R object of lmerMod class}
  \item{contrastOpt}{contrast option}
  \item{controlLevel}{control level}
  \item{contrast}{contrast matrix}
  \item{alpha}{level of significance}
}
\author{
 AAGulles
}
\keyword{analysis}

