\name{GETwoStage.dataRecode}
\alias{GETwoStage.dataRecode}
\alias{GETwoStage.dataRecode.default}
\title{GETwoStage.dataRecode}
\description{
This function performs data recoding for genotype and environment columns for Multi-Env two stage analysis
}
\usage{
GETwoStage.dataRecode(data, respvar, geno, env)
}
\arguments{
  \item{data}{data frame}
  \item{respvar}{vector of strings; variable names of the response variables}
  \item{geno}{string; name of genotype factor}
  \item{env}{string; name of environment factor}
}
\author{
Nellwyn Sales
}
\keyword{ dataRecode }