\name{generationMeanSummaryStats.test}
\alias{generationMeanSummaryStats.test}
\alias{generationMeanSummaryStats.test.default}
\title{generationMeanSummaryStats.test}
\description{
This function uses Weighted Least Square Method Regression
}
\usage{
generationMeanSummaryStats.test(data, meanVar, weightsVar, stdDevVar, numObsVar, generationVar, usersNotationVector, generalNotationVector, alpha)
}
\arguments{
  \item{data}{a dataframe that contains summary statistics of the generations}
  \item{meanVar}{string; name of the variable for the generation means}
  \item{weightsVar}{string; name of the variable for the generation weights}
  \item{stdDevVar}{string; name of the variable for the generation standard deviations}
  \item{numObsVar}{string; name of the variable for the number of observations per generation}
  \item{generationVar}{string; name of the variable for the generation}
  \item{usersNotationVector}{a vector of the user's notation for different generations}
  \item{generalNotationVector}{a vector of the general notation for different generations}
  \item{alpha}{a string, user's desired level of significance}
}
\author{
Nellwyn L. Sales
}
\keyword{gma}